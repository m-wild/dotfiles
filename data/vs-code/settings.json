{
    "terminal.external.windowsExec": "powershell",
    "terminal.integrated.shell.windows": "\\windows\\system32\\WindowsPowerShell\\v1.0\\powershell.exe",
    "files.associations": {
        "*.conf": "ini"
    },
    "mssql.connections": [
        {
            "server": "(localdb)\\mssqllocaldb",
            "database": "cashmoney",
            "authenticationType": "Integrated",
            "profileName": "cashmoney@localdb",
            "password": ""
        },
        {
            "server": "(localdb)\\mssqllocaldb",
            "database": "sandpit",
            "authenticationType": "Integrated",
            "profileName": "sandpit@localdb",
            "password": ""
        }
    ],
    "mssql.resultsFontSize": 14,
    "mssql.resultsFontFamily": "Arial",
    "git.confirmSync": false,
    "window.zoomLevel": -0.4,
    "editor.fontSize": 13,
    "workbench.editor.showIcons": false,
    "editor.wordWrap": true,
    "window.menuBarVisibility": "toggle",
    "workbench.welcome.enabled": false,
    "editor.renderWhitespace": "all",
    "workbench.colorTheme": "One Dark Pro",
    "workbench.iconTheme": "vs-seti",
    "editor.minimap.enabled": true,
    "auto-close-tag.activationOnLanguage": [
        "html",
        "xml"
    ],
    "git.enableSmartCommit": true
}